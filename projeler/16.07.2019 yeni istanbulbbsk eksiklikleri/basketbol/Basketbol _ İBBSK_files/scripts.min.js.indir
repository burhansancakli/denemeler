        top = (window.outerHeight - height) / 2,
        url = window.location,
        opts = 'resizable=1,scrollbars=1,status=1' +
            ',width=' + width +
            ',height=' + height +
            ',top=' + top +
            ',left=' + left;
    var sharingUrl = ("https://www.facebook.com/sharer/sharer.php?u=" + url + "&text=" + page_title);
    var newwindow = window.open(sharingUrl, "", opts);
}

function postToTWTTR() {
    console.log($('#share_title'));
