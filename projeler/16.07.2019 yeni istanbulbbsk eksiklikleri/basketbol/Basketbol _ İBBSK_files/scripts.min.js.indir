        click(event, context) {
            if (context.element.val() !== '') {
                context.element.parent('label').addClass('active');
            } else {
                context.element.parent('label').removeClass('active');
            }
        }

    });
    CalendarSelect.on('change', function () {
        if ($(this).val() !== '0') {
            $(this).parent('label').addClass('active');
        } else {
            $(this).parent('label').removeClass('active');
        }
    });
    CalendarSearch.on("keyup", function () {
        if ($(this).val() !== '') {
            $(this).parent('label').addClass('active');
        } else {
            $(this).parent('label').removeClass('active');
        }
    });
    if (bodyId === 'activities-page') {
        const Calendar = $('#activity-calendar');
        const CalendarDays = $('#calendar-days');
        const CalenderSubmit = $('#activities-filter button');
        const DateTime = Calendar.find('#date time');
        const CalendarMonth = Calendar.find('#month li');
        const PostDate = $('#post-date');
        const CalendarOpen = $('.calendar-open');
        CalenderSubmit.on('click', function (e) {
            const data = $('#activities-filter form').serializeArray();
            $.ajax({
                url: '',
                method: 'GET',
                dataType: 'json',
                data: data,
                success: function (res) {

                }, error: function (res) {
                }
            });
        });
        const monthNames = ['Ocak', 'Şubat', 'Mart', 'Nisan', 'Mayıs', 'Haziran', 'Temmuz', 'Ağustos', 'Eylül', 'Ekim', 'Kasım', 'Aralık'];
        const daysNames = ['Pazartesi', 'Salı', 'Çarşamba', 'Perşembe', 'Cuma', 'Cumartesi', 'Pazar'];
        CalendarDays.pignoseCalendar({
            lang: 'tr',
            format: 'DD-MM-YYYY',
            week: 1,
            weeks: [
                'Paz',
                'Pzt',
                'Sal',
                'Çar',
                'Per',
                'Cum',
                'Cmt',
            ],
            schedules: eventActives,
            scheduleOptions: {
                colors: {
                    event: '#ffffff',
                }
            },
            select: function (date, content) {
                if (date[0]._f && date[0]._i) {
                    const selectMonth = date[0]._pf.parsedDateParts[1];
                    let selectDay = ('0' + date[0]._pf.parsedDateParts[2]).slice(-2);
                    let dt = new Date(date[0]._i).getDay() - 1;
                    if (dt === -1) {
                        dt = 6;
                    }
                    DateTime.html(selectDay + ' ' + monthNames[selectMonth] + ' ' + '<span>' + daysNames[dt] + '</span>');
                    $('#month li[data-month=' + selectMonth + ']').addClass('active');
                    DateTime.attr('date', date[0]._i);
                }
            },
            init: function (context) {
                DateTime.attr('date', context.dateManager.year + '-' + ('0' + context.dateManager.month).slice(-2) + '-' + ('0' + context.dateManager.day).slice(-2));
                DateTime.attr('lastDay', ('0' + context.dateManager.lastDay).slice(-2));
                DateTime.attr('firstDay', ('0' + context.dateManager.firstDay).slice(-2));
                DateTime.attr('month', context.dateManager.month);
            }
        });
        Calendar.find('#date .button').on('click', function (e) {
            let toggleDate;
            const dateTimeSplit = parseInt(DateTime.attr('date').split('-')[2]);
            const clickPrev = dateTimeSplit - 1;
            const clickNext = dateTimeSplit + 1;
            if (e.currentTarget.className === 'button prev') {
                toggleDate = clickPrev;
                if (toggleDate < DateTime.attr('firstDay')) {
                    toggleDate = DateTime.attr('firstDay');
                }
            } else if (e.currentTarget.className === 'button next') {
                toggleDate = clickNext;
                if (toggleDate > DateTime.attr('lastDay')) {
                    toggleDate = DateTime.attr('lastDay');
                }
            }
            PostDateResult();
            setTimeout(function () {
                PostDate.trigger('click');
            }, 300);
            CalendarDays.pignoseCalendar('set', toggleDate.toString());
            DateTime.attr('date', $('.pignose-calendar-unit-first-active').attr('data-date'));
            let dt = new Date(DateTime.attr('date')).getDay() - 1;
            if (dt === -1) {
                dt = 6;
            }
            DateTime.html(toggleDate + ' ' + monthNames[DateTime.attr('month') - 1] + ' ' + '<span>' + daysNames[dt] + '</span>');
        });
        CalendarMonth.on('click', function () {
            const thisMonth = $(this).data('month');
            DateTime.attr('date', thisMonth);
            DateTime.attr('month', thisMonth.split('/')[1]);
            CalendarDays.pignoseCalendar('set', thisMonth.toString());
            CalendarMonth.removeClass('active');
            $(this).addClass('active');
        });
        CalendarOpen.on('click', function () {
            $(this).removeClass('active');
            $('#con_one').removeClass('active');
            $('#con_two').addClass('active');
            CalendarDays.show();
            PostDate.show();
        });

        function PostDateResult() {
            $('.result-listing li.activity').hide().attr('status', '');
            $('.result-listing li[data-date=' + DateTime.attr('date') + ']').show().attr('status', 'open');
            $('.result.length').text('Bu tarihte toplam ' + $('.result-listing li[status="open"]').length + ' etkinlik bulunmaktadır')
        }

        //PostDateResult();

        PostDate.on('click', function () {
            CalendarDays.hide();
            $('#con_two').removeClass('active');
            $('#con_one').addClass('active');
            CalendarOpen.addClass('active');
            PostDateResult();
            $(this).hide();
        });
    }
});

