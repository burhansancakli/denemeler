$(document).ready(function () {
    function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, '\\$&');
        const regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, ' '));
    }

    const body2 = $('body');
    const preLoader = $('.preloader');
    const headerBox = $('header .box');
    const infoBar = $('header .info-bar');
    const filters = $('.page-wrapper section');
    const filterDropDown = $('.filter-dropdown');
    const filterActive = $('.filter-dropdown .title b');
    const filterItems = $('.filter-dropdown ul');
    const filterItem = $('.filter-dropdown li');
    const HeadTabsTitle = $('.head-links .head-mobile-btn');
    const HeadTabsItem = $('.head-links a');
    const FilterSearch = $('.filter-search');
    const activitiesBranchSlider = $('.activities-branch-slider');
    const navTextIcon = $('.nav-text-icon');
    const mobileNavIcon = $('.mobile-nav-icon');
    const mobileMenu2 = $('#menu-wrapper .mobile-menu');
    const menuClose = $('#menu-wrapper .menu-close');
    const fixedSearch = $('.fixed-search');
    const websiteSearch = $('input#website_search');
    const branchMobileNavIcon = $('.branch-mobile-nav-icon');
    $('header .contact-us').on('click', function () {
        if (headerBox.hasClass('hide')) {
            headerBox.removeClass('hide');
        } else {
            headerBox.addClass('hide');
        }
        if ($(this).hasClass('disable') === true) {
            $(this).removeClass('disable');
        } else {
            $(this).addClass('disable');
        }
    });
    $('.circle-contact').on('click', function () {
        infoBar.addClass('active');
    });
    $('[data-fancybox]').fancybox({
        buttons: [
            //"zoom",
            //"share",
            "slideShow",
            "fullScreen",
            //"download",
            //"thumbs",
            "close"
        ],
    });
    HeadTabsTitle.on('click', function () {
        if ($(this).next('.links').css('display') === 'block') {
            $(this).find('i').removeClass('fa-angle-up');
            $(this).find('i').addClass('fa-angle-down');
            $(this).next('.links').hide();
        } else {
            $(this).find('i').removeClass('fa-angle-down');
            $(this).find('i').addClass('fa-angle-up');
            $(this).next('.links').show();
        }
    });
    filterDropDown.on('click', function () {
        if (filterItems.css('display') === 'none') {
            filterItems.fadeIn();
        } else {
            filterItems.fadeOut();
        }
    });
    filterItem.on('click', function () {
        const dataId = $(this).data('id');
        const dataTitle = $(this).text();
        const dataBranch = $(this).data('branch');
        const dataLink = $(this).data('href');
        filterActive.parent('.title').attr('data-active', dataId);
        filterActive.html(dataTitle);
        $('.score-table').removeClass('active');
        $('.score-table[data-id=' + dataId + ']').addClass('active')
        $('#breadcrumbBranch').text(dataBranch)
        $('#dataLeague').text(dataTitle)
        urlHost = document.location.protocol + '//' + document.location.host;
        $('#breadcrumbBranch').attr('href', urlHost + dataLink)
    });
    HeadTabsItem.on('click', function () {
        const dataId = $(this).data('id');
        const dataTitle = $(this).text();
        HeadTabsTitle.attr('data-active', dataId);
        HeadTabsTitle.html(dataTitle + ' <i class="fa fa-angle-down"></i>');
        if (filters.width() <= 791) {
            $(this).parent('.links').hide();
        } else {
            $(this).parent('.links').show();
            HeadTabsItem.removeClass('active');
            $(this).addClass('active');
        }
    });
    $('.newsletters-branch').slick({
        dots: false,
        arrows: true,
        infinite: false,
        centerPadding: '0',
        speed: 1000,
        autoplay: true,
        autoplaySpeed: 4000,
        slidesToShow: 2.2,
        slidesToScroll: 1,
        swipeToSlide: true,
        adaptiveHeight: true,
        responsive: [
            {
                breakpoint: 768,
                settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1,
                    dots: true,
                }
            }
        ]
    });
    $('#custom-search').on('click', function () {
        FilterSearch.toggleClass('active');
    });
    $('.featured-video .play').on('click', function () {
        $(this).parent('.content').fadeOut();
    });
    activitiesBranchSlider.slick({
        slidesToShow: 5.6,
        slidesToScroll: 1,
        dots: false,
        infinite: false,
        speed: 300,
        responsive: [
            {
                breakpoint: 1200,
                settings: {
                    slidesToShow: 4.6,
                    slidesToScroll: 1,
                }
            },
            {
                breakpoint: 991,
                settings: {
                    slidesToShow: 3.6,
                    slidesToScroll: 1,
                }
            },
            {
                breakpoint: 767,
                settings: {
                    slidesToShow: 2.6,
                    slidesToScroll: 1,
                }
            },
            {
                breakpoint: 600,
                settings: {
                    slidesToShow: 1.6,
                    slidesToScroll: 1,
                }
            },
            {
                breakpoint: 450,
                settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1,
                }
            }
        ]
    });
    if (screen.width <= 991) {
        branchMobileNavIcon.parent('a').parent('.logo').siblings('nav').slideUp();
    }
    branchMobileNavIcon.on('click', function () {
        $('#vertical-menu').find('nav').slideToggle();
    });
    const mobileMenu = $('#mobile-menu');
    const mobileStatic = mobileMenu.children('.static');
    const mobileDynamic = mobileMenu.find('.dynamic');
    const mobileToggle = mobileMenu.find('.toggle,.search');
    const menuItem = mobileMenu.find('.menu-item');
    const menu = $('.menu-item.menu');
    const menuClubs = $('.menu-item.clubs');
    const menuMedia = $('.menu-item.mobile-media:not(.clubs)');
    const menuBranch = $('.menu-item.mobile-branch');
    const menuLink = menu.find('a');
    const search = $('.menu-item.mobile-search');
    const menuFoot = mobileMenu.find('.foot');
    const mobileSearch = $('#mobile_search');
    const results = mobileMenu.find('.results');
    const customSearchOpen = $('.custom-search-open');
    const searchFilter = mobileMenu.find('.filter');
    const mobileSearchItem = searchFilter.find('li');
    const calendarAdd = $('.calendar-add');
    const popup = $('#popup');
    const teamPlayers = $('#team-page .players li');

    mobileToggle.on('click', function (e) {
        menuItem.removeClass('active');
        results.removeClass('active');
        mobileSearch.val('');
        if (e.currentTarget.className === 'search') {
            search.addClass('active');
            menuFoot.addClass('hidden');
        }
        if (e.currentTarget.className === 'toggle') {
            menu.addClass('active');
            menuFoot.removeClass('hidden');
        }
        if (mobileMenu[0].className !== 'active') {
            $('html,body').addClass('disable');
            mobileToggle.find('.close-menu').show();
            mobileMenu.addClass('active');
            mobileStatic.addClass('hidden');
            mobileDynamic.addClass('active');
            results.removeClass('active');
            searchFilter.removeClass('active');
            customSearchOpen.addClass('active');
        } else {
            $('html,body').removeClass('disable');
            mobileMenu.removeClass('active');
            mobileToggle.find('.close-menu').hide();
            mobileStatic.removeClass('hidden');
            mobileDynamic.removeClass('active');
            customSearchOpen.removeClass('active');
        }
    });
    mobileSearch.on('keyup', function (event) {
        if (event.currentTarget.value) {
            results.addClass('active');
            customSearchOpen.removeClass('active');
            searchFilter.removeClass('active');
        } else {
            customSearchOpen.addClass('active');
            results.removeClass('active');
        }
    });
    mobileSearchItem.on('click', function (e) {
        if (e.currentTarget.className === 'active') {
            $(this).removeClass('active');
        } else {
            $(this).addClass('active');
        }
    });
    menuLink.on('click', function () {
        const dataBox = $(this).data('box');
        menuItem.removeClass('active');
        if (dataBox === 'clubs') {
            menuClubs.addClass('active');
        } else if (dataBox === 'media') {
            menuMedia.addClass('active');
        } else if (dataBox === 'branch') {
            menuBranch.addClass('active');
        }

    });
    customSearchOpen.on('click', function () {
        searchFilter.addClass('active');
        $(this).removeClass('active');
    });
    $('.close-register').on('click', function () {
        $('.contact-mobile').addClass('disabled');
    });
    $('.contact-open').on('click', function () {
        $('.contact-mobile').removeClass('disabled');
    });
    calendarAdd.on('click', function () {
        const googleDate = $(this).data('date');
        const title = $(this).data('title');
        const branch = $(this).data('branch');
        popup.find('.button').attr('href', 'http://www.google.com/calendar/event?action=TEMPLATE&text=' + title + ' - ' + branch + '&dates=' + googleDate + '/' + googleDate + '')
        popup.removeClass('hidden');
        popup.find('.default').show();
        popup.find('.success').hide();
    });
    popup.find('.close-popup').on('click', function () {
        popup.addClass('hidden');
    });

    $(document).mouseup(function (e) {
        activitePopup = $('#activities-page #popup');
        activitePopupDefault = $('#activities-page #popup .default');
        if (activitePopupDefault) {
            if (!activitePopupDefault.is(e.target) && activitePopupDefault.has(e.target).length === 0) {
                activitePopup.addClass('hidden');
            }
        }
    });

    HeadTabsItem.on('click', function () {
        $('.team-content .players p').remove();
        const thisPosition = $(this).data('position');
        teamPlayers.hide();
        if (thisPosition === 'all') {
            teamPlayers.show();
        } else {
            $('#team-page .players li[data-position=' + thisPosition + ']').show();
        }
        mess = true
        $('.team-content .players li').each(function (index, val) {
            if ($(this).css('display') === "none") {

            } else {
                mess = false
            }
        })
        if (mess === true) {
            setTimeout(function () {
                $('.team-content .players').append("<p style='width: 100%; text-align: center;margin-top:60px;'>Bu Pozisyon da Hiç Kimse Bulunamadı !</p>")
            }, 300)

        }

    });

    function create_bulletin(input_elem, message_elem) {
        $.ajax({
            url: "bulten/",
            type: "POST",
            data: {email: input_elem.val()},
            success: function (data, i) {
                input_elem.val('');
                if (data[0].text.email) {
                    message_elem.html("<p style='visibility: visible'>" + data[0].text.email + "</p>");
                } else {
                    message_elem.html("<p style='visibility: visible'>" + data[0].text + "</p>");

                }
            },
            error: function (xhr, errmsg, err) {
                message_elem.html("<p style='visibility: visible'>Bir sorun oluştu! Lütfen daha sonra tekrar deneyiniz.</p>");
                console.log(xhr);
            }
        })
    }

    function create_contact() {
        var data = $('#contact-form').serializeArray();
        $.ajax({
            url: "mesaj/",
            type: "POST",
            data: data,
            success: function (json) {
                console.log(json);
                $('.contact-message').html("<p>Mesajınız Gönderildi! İlginiz için Teşekkürler.</p>");
            },
            error: function (xhr, errmsg, err) {
                $('.contact-message').html("<p>Oops! Mesajınız Gönderilemedi.</p>");
                console.log(xhr.status + ": " + xhr.responseText)
            }
        })
    }

    const headerBullettin = $('.header-bulletin');
    const headerBullettinMobile = $('.header-bulletin-mobile');
    const bullettinMessageHeader = $('.bulletin-message_header');
    const footerBullettin = $('.footer-bulletin');
    $('#header-bulletin').on('submit', function (e) {
        e.preventDefault();
        headerBullettin.val();
        create_bulletin($('.header-bulletin'), bullettinMessageHeader);
    });

    $('#header-bulletin-mobile').on('submit', function (e) {
        e.preventDefault();
        headerBullettinMobile.val();
        create_bulletin(headerBullettinMobile, bullettinMessageHeader);
    });

    $('#footer-bulletin').on('submit', function (e) {
        e.preventDefault();
        footerBullettin.val();
        create_bulletin(footerBullettin, $('.bulletin-message_footer'));
    });

    $('#contact-form').on('submit', function (e) {
        e.preventDefault();
        create_contact();
    });


    const videoPromotional = $('[data-promotional]');
    const promotionClick = $('#promotional-film .video-cover');
    videoPromotional.on('click', function () {
        if ($('body').attr('id') === 'home-page') {
            $('html, body').animate({scrollTop: promotionClick.offset().top - 100}, 2000);
            setTimeout(function () {
                promotionClick.click();
            }, 2500);

        } else {
            setTimeout(function () {
                promotionClick.click();
            }, 1000);
        }
    });

    $('#share_on_twitter').on('click', function () {
        postToTWTTR();
    });
    $('#share_on_facebook').on('click', function () {
        var page_title = $('.page-title h1').text();
        postToFB(page_title);
    });

    let title = $('#share_title').text();
    if (title == '' || title == 'undefinied')
        title = $.trim($("title").text());
    title = 'İBBSK | ' + title;

    const body = encodeURIComponent(location.href);
    $('#share_via_mail').attr('href', `mailto:?subject=${title}&body=${body}`);


    $('.paragraph p').each(function () {
        if ($(this).text() === " " || $(this).text() === "") {
            $(this).remove();
        }
    });

    const search_button = $('.search-button');
    const searchClose = $('.search-close');
