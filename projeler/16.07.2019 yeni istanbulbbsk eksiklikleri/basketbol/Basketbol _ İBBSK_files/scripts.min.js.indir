                         keys.forEach(k => {
                             result += `<li><a href="${res[k].url}" title="${res[k].title}">
                                             <b>${res[k].tag}${res[k].title}</b>
                                         </a>
                                     </li>`;
                         });
                         Results.find('ul').html(result);

                     }
                 });

             } else if ($(this)[0].id === 'mobile_search') {
                 Results = $('.results');
                 $.ajax({
                     url: "/arama/search?term=" + thisValues,
                     method: 'GET',
