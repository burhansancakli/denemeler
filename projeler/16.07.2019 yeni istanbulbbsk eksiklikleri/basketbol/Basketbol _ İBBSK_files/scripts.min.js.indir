    body2.css({"visibility": "visible", "opacity": "1"});
    setTimeout(function () {
        preLoader.hide();
    }, 3000);
    searchClose.on('click', function () {
        fixedSearch.removeClass('open')
        body2.css({'overflow': 'auto'})
        websiteSearch.val('');
        $('#results').addClass('hidden').children('ul').find('li').remove()
    });

    function autoHeight_all_view() {
        const newsListWrapper = $('.news-list-wrapper');
        const buttonHeight = $('.all-views').height();
        const titleHeight = $('.news-list-vertical-title').height();
