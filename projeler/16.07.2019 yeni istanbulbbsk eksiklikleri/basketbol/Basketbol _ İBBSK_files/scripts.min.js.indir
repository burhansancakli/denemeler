$(document).ready(function () {
    const galleryCategory = $('.gallery-content button');
    const containerPhoto = $('.gallery-images');
    let count = 1;
    let orderBy = $('.head-links .links a.active').data('ordering');
    let data_url;

    galleryCategory.on('click', function () {
        // galleryCategory.removeClass('active');
        // $(this).addClass('active');
        data_url = `/api/gallery-list?page_size=20&&ordering=${orderBy}&&page=`;
        $.ajax({
            url: data_url + count.toString(),
            method: 'GET',
            dataType: 'json',
            success: function (res) {
                let result = '';

                res.results.forEach(function (item) {
                    if (!item.image.length < 1) {
                        result += `<li>
                            <a href="${item.url}" title="${item.title}">
                                <figure style="background-image:url(${item.image})">
                                    <figcaption>${item.title}</figcaption>
                                </figure>
                            </a>
                        </li>`
                    }
                });
                count += 1;
                containerPhoto.append(result);
                if (!res.next) {
                    $(galleryCategory).css('display', 'None');
                }
            }, error: function (res) {
            }
        });
    });

    setTimeout(function () {
        galleryCategory.trigger('click');
    }, 1500);

    $('.head-links .links a').on('click', function () {
        let newOrderBy = $('.head-links .links a.active').data('ordering');
        if (newOrderBy != orderBy) {
            orderBy = newOrderBy;
            containerPhoto.html('');
            count = 1;
        }
        galleryCategory.trigger('click');
    });
});
