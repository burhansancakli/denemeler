            if (content.is(':visible')) {
                $('body').css('overflow', 'hidden');
                console.log('deneme')
            } else {
                $('body').css('overflow', 'auto');
            }
        })
    });

    const icon_list = $('.search-wrapper .icon-list');
    icon_list.hide()
    $('.custom-search a').on('click', function () {
        if ($('.custom-search a').css("opacity") === '0.25') {
            $('.custom-search a').css('opacity', '1');
            icon_list.fadeToggle();
        } else {
            $('.custom-search a').css('opacity', '0.25');
            icon_list.fadeToggle();
        }
    });

    const icon_list_wrapper = $('.search-wrapper .icon-list .icon-wrapper');
    icon_list_wrapper.on('click', function () {
        if ($(this).hasClass('active')) {
            $(this).removeClass('active')
        } else {
            $(this).addClass('active')
        }

    });

    $('.text-search input').keyup(function () {
        if ($('.text-search input').val() != '') {
            icon_list.hide()
            $('.custom-search a').hide()
            $('.search-result').show();

        } else {
            $('.custom-search a').show()
            $('.search-result').hide()
        }
    });


    $('.vertical-menu nav a').hover(function () {
        var content = $('.menu-content')
        if (content.is(':visible')) {
            $('body').css('overflow', 'hidden');
            console.log('deneme')
        } else {
            $('body').css('overflow', 'auto');
        }
    })

    $('.search a').click(function () {
        $('.search-close a').click(function () {
            content.css('display', 'none');
            $('body').css('overflow', 'auto');
        });
    })

});
