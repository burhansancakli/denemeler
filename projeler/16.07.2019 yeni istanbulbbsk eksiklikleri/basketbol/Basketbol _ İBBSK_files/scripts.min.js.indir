    const search_input = $('#menu-wrapper .search-wrapper form label input');
    search_button.on('click', function () {
        fixedSearch.addClass('open');
        body2.css({'overflow': 'hidden'})
    });
    mobileNavIcon.click(function () {
        if (mobileMenu2.hasClass('open')) {
            mobileMenu2.removeClass('open');
            body2.css('overflow', 'auto');
        } else {
            mobileMenu2.addClass('open');
            body2.css('overflow', 'hidden');
        }
    });
    menuClose.click(function () {
        if (mobileMenu2.hasClass('open')) {
            mobileMenu2.removeClass('open');
            body2.css('overflow', 'auto');
            navTextIcon.removeClass('open');
            mobileMenu2.find('.sub-open-menu').removeClass('open');
        }
    });
    navTextIcon.click(function () {
        if ($(this).hasClass('open')) {
            $(this).removeClass('open');
            $(this).parent('.nav-item').find('.sub-open-menu').removeClass('open');
        } else {
            $('.nav-text-icon.open, .sub-open-menu.open').removeClass('open');
            $(this).addClass('open');
            $(this).parent('.nav-item').find('.sub-open-menu').addClass('open');
