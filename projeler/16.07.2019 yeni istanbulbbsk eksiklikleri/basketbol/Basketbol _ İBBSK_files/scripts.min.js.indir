                                            <div class="content">
                                                <div class="top">${branch_elem}</div>
                                                <div class="play" title="Başlat"><img
                                                        src="/static/img/icons/youtube-icon.svg" alt="youtube-icon">
                                                </div>
                                                <div class="title">${item.title}</div>
                                            </div>
                                        </a>
                                   </li>`;
                    }
                });
                count += 1;
                Videocontainer.append(result);
                if (!res.next) {
                    videos_button.hide();
                }
            }, error: function (res) {
            }
        });
    }

    function loadAll() {
        $.ajax({
            url: '/api/youtube-videos/?page_size=12&&page=' + count.toString(),
            method: 'GET',
            dataType: 'json',
            success: function (res) {
                res.results.forEach(function (item) {
                    result += `<li>
                                        <a href="https://www.youtube.com/watch?v=${item.video_id}"
                                           title="${item.title}" data-fancybox
                                           style="background-image: url('https://img.youtube.com/vi/${item.video_id}/hqdefault.jpg')">
                                            <div class="content">
                                                <div class="top"></div>
                                                <div class="play" title="Başlat"><img
                                                        src="/static/img/icons/youtube-icon.svg" alt="youtube-icon">
                                                </div>
                                                <div class="title">${item.title}</div>
                                            </div>
                                        </a>
                                   </li>`;
                });
                count += 1;
                Videocontainer.append(result);
                if (!res.next) {
                    videos_button.hide();
                }
            }, error: function (res) {
            }
        });
    }

    setTimeout(function () {
        url = $('.links a.active').data('url');
        videos_button.trigger('click');
    }, 1500);

    $('.links a').on('click', function () {
        new_url = $('.links a.active').data('url');
        if (new_url != url) {
            Videocontainer.html('');
            videos_button.show();
            url = new_url;
            count = 1;
            videos_button.trigger('click');
        }
    });
});


