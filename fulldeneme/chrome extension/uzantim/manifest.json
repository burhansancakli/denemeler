{
  "manifest_version": 2,
  "name": "Benim Extension",
  "version": "1",
  "description": "Deneme amaçlı, bi çok şey yapar herhalde ilerde",
  "author": "Brand0",
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "background": {
    "persistent": false,
    "scripts": [ "background.js" ]
  },
  "browser_action": {
    "default_title": "browser_action.title",
    "default_popup": ""
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      }
    }
  }
}
